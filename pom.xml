        <project.build.skip.deploy.maven-site-plugin>false</project.build.skip.deploy.maven-site-plugin>
        <project.build.skip.maven-source-plugin>false</project.build.skip.maven-source-plugin>

        <!-- Maven Site directory settings. -->
        <project.build.site.directory>${project.basedir}/src/site</project.build.site.directory>
