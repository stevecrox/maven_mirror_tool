        <project.analyse.fail.on.error>false</project.analyse.fail.on.error>
        <project.analyse.fail.on.error.maven-changelog-plugin>${project.analyse.fail.on.error}</project.analyse.fail.on.error.maven-changelog-plugin>
        <project.analyse.fail.on.error.maven-checkstyle-plugin>${project.analyse.fail.on.error}</project.analyse.fail.on.error.maven-checkstyle-plugin>
        <project.analyse.fail.on.error.maven-javadoc-plugin>${project.analyse.fail.on.error}</project.analyse.fail.on.error.maven-javadoc-plugin>
        <project.analyse.fail.on.error.findbugs-maven-plugin>${project.analyse.fail.on.error}</project.analyse.fail.on.error.findbugs-maven-plugin>

        <!-- Gives Ability to skip specific plugins and stages. -->
        <project.analyse.skip>false</project.analyse.skip>
        <maven.analysis.include.tests>false</maven.analysis.include.tests>
        <project.analyse.skip.maven-changelog-plugin>${project.analyse.skip}</project.analyse.skip.maven-changelog-plugin>
        <project.analyse.skip.maven-checkstyle-plugin>${project.analyse.skip}</project.analyse.skip.maven-checkstyle-plugin>
